node{
 
 stage('SCM Checkout'){
git credentialsId: 'github', url: 'https://github.com/shivastunts0327/jobhub.git'
}
   
  stage('SonarQube analysis') {
    def scannerHome = tool 'sonarqube';
           withSonarQubeEnv("sonarqube") {
           sh "${scannerHome}/bin/sonar-scanner \ 
          -Dsonar.projectKey=ihscareers \
          -Dsonar.sources=. \
          -Dsonar.host.url=http://13.235.16.197:9000 \"
  }
  }

}
