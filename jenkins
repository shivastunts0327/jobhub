node{
   
   stage('SCM Checkout'){
   git credentialsId: 'github', url: 'https://github.com/shivastunts0327/jobhub.git'
   }
   
   
     stage('nodejs Package'){
     nodejs(cacheLocationStrategy: workspace(), nodeJSInstallationName: 'nodejs') {    
     sh "npm install" 
     sh "npm run build" 
     sh "npm run dev-server" 
     }
   } 
   
   }
